exec swaymsg "workspace $ws1; exec firefox; workspace $ws1;"                                                                                                                                                                   │" Press ? for help             
exec swaymsg "workspace $ws2; exec kitty; workspace $ws1;"                                                                                                                                                                     │                               
exec swaymsg "workspace $ws3; exec kitty --hold nnn; workspace $ws1;"                                                                                                                                                          │.. (up a dir)                  
exec swaymsg "workspace $ws3; exec kitty --hold htop; workspace $ws1;"                                                                                                                                                         │/home/pietryszak/              
exec swaymsg "workspace $ws3; exec kitty; workspace $ws1;"                                                                                                                                                                     │▸ bhbikljkij/                  
exec swaymsg "workspace $ws4; exec caprine; workspace $ws1;"                                                                                                                                                                   │▸ bin/ -> /usr/local/bin/      
exec swaymsg "workspace $ws5; exec thunderbird; workspace $ws1;"                                                                                                                                                               │▸ Desktop/                     
exec sleep 10 && exec swaymsg "workspace $ws6; exec code; workspace $ws1;"                                                                                                                                                      │▸ Documents/                   
exec sleep 14 && exec swaymsg "workspace $ws9; exec spotify; workspace $ws1;"
